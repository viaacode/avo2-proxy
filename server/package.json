{
  "name": "avo2-proxy-server",
  "version": "0.10.0",
  "description": "Deze repo bevat de NodeJS service die de communicatie regelt tussen de Archief voor Onderwijs frontend applicaties enerzijds en de backend services anderzijds.",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viaacode/avo2-proxy.git"
  },
  "author": "Bert Verhelst <bert.verhelst@studiohyperdrive.be>",
  "contributors": [
    "Enzo Eghermanne <enzo.eghermanne@studiohyperdrive.be>"
  ],
  "homepage": "https://github.com/viaacode/avo2-proxy",
  "scripts": {
    "start": "npm run serve",
    "serve": "ts-node-dev --transpileOnly --poll -- index.ts",
    "prebuild": "npm run clean:build",
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json -p tsconfig.json -t stylish",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run clean:coverage",
    "test": "jest --config jest.config.js --runInBand",
    "test:watch": "npm run test -- --watchAll",
    "posttest": "npm run lint",
    "clean:build": "rm -rf ./dist",
    "clean:coverage": "rm -rf ./test/coverage",
    "open:coverage": "open ./test/coverage/lcov-report/index.html"
  },
  "dependencies": {
    "@hapi/joi": "15.1.1",
    "@studiohyperdrive/logger": "1.1.1",
    "@types/hapi__joi": "15.0.3",
    "@viaa/avo2-types": "^2.2.1",
    "apollo-boost": "^0.4.4",
    "async": "3.1.0",
    "axios": "0.19.0",
    "blend-promise-utils": "1.24.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.4",
    "cors": "^2.8.5",
    "elasticsearch-sanitize": "2.0.0",
    "express": "4.17.1",
    "express-session": "1.15.6",
    "express-useragent": "^1.0.13",
    "graphql": "^14.4.2",
    "helmet": "3.20.0",
    "lodash": "^4.17.15",
    "node-cron": "^2.0.3",
    "passport-saml": "^1.1.0",
    "public-ip": "^3.2.0",
    "query-string": "^6.8.1",
    "ramda": "0.26.1",
    "reflect-metadata": "0.1.13",
    "remove-accents": "0.4.2",
    "saml2-js": "^2.0.3",
    "sanitize-html": "^1.20.1",
    "session-file-store": "1.2.0",
    "swagger-express-ts": "1.0.1",
    "swagger-ui-express": "4.0.7",
    "typescript-rest": "2.2.2",
    "uuid": "3.3.3",
    "xml-js": "1.6.11"
  },
  "devDependencies": {
    "@types/async": "3.0.1",
    "@types/axios": "0.14.0",
    "@types/body-parser": "1.17.1",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.5",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.1",
    "@types/express-session": "^1.15.11",
    "@types/express-useragent": "^0.2.22",
    "@types/hapi__joi": "^15.0.3",
    "@types/helmet": "0.0.44",
    "@types/jest": "24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "12.7.2",
    "@types/node-cron": "^2.0.2",
    "@types/ramda": "0.26.20",
    "@types/saml2-js": "^1.6.7",
    "@types/sinon": "7.0.13",
    "@types/sinon-express-mock": "1.3.7",
    "@types/supertest": "2.0.8",
    "@types/swagger-ui-express": "3.0.1",
    "@types/uuid": "3.4.5",
    "dotenv": "8.1.0",
    "jest": "24.8.0",
    "jest-extended": "0.11.2",
    "sinon": "7.4.1",
    "sinon-express-mock": "2.2.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node-dev": "1.0.0-pre.42",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "typescript": "3.5.2"
  },
  "license": "ISC"
}
